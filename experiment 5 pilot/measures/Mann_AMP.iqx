***********************************************************************
Trials
***********************************************************************
<trial prime_positive>
/ validresponse = ("D", "K")
/ correctresponse = ("K")
/ stimulustimes = [0= cross; 500 = prime_positive; 575=blank; 675=target; 775=mask]
/ontrialend = [values.primestim = picture.prime_positive.currentitemnumber; values.targetstim = picture.target.currentitemnumber]
</trial>

<trial prime_negative>
/ validresponse = ("D", "K")
/ correctresponse = ("K")
/ stimulustimes = [0= cross; 500 = prime_negative; 575=blank;  675=target; 775=mask]
/ontrialend = [values.primestim = picture.prime_negative.currentitemnumber; values.targetstim = picture.target.currentitemnumber]
</trial>


***********************************************************************
Blocks
***********************************************************************

<block instructions>
/ bgstim = (attributeAlabel, attributeBlabel)
/ trials = [1-3 = instructions; 4 = Mann_instruction]
</block>

<block test>
/ bgstim = (attributeAlabel, attributeBlabel)
/ trials = [1-60 = noreplace(prime_positive, prime_negative)]
</block>


***********************************************************************
Experiment
***********************************************************************
<expt>
/ blocks = [1=instructions; 2=test]
/onexptend = [values.completed = 1]
</expt>

***********************************************************************
Test Monkey
***********************************************************************
<monkey>
/ latencydistribution = normal(1, 1)
/ percentcorrect = 50
</monkey>


