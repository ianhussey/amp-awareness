<picture target>
/ items = symbols
/ select = noreplace
/ size = (30%, 30%)
</picture>

<shape blank>
/ shape = rectangle
/ color = black
/ size = (30%, 30%)
</shape>

<picture prime_trump>
/ items = prime_trump
/ size = (30%, 30%)
</picture>

<picture prime_obama>
/ items = prime_obama
/ size = (30%, 30%)
</picture>

<text attributeAlabel>
/ items = attributeAlabel
/ valign = top
/ halign = right
/ position = (95%, 5%)
/ txcolor = green
/ fontstyle = ("Arial", 5%)
